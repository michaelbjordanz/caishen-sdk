{
  "name": "@caishen/sdk",
  "version": "1.0.3",
  "type": "module",
  "description": "The Caishen SDK gives every agent or user access to unlimited multi-chain crypto wallets",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "mocha --timeout 30000 --import=tsx",
    "test:all": "mocha --timeout 30000 --import=tsx 'test/**/*.spec.ts'",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap --minify"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Caishen <hello@caishen.tech> (https://caishen.tech)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CaishenTech/caishen-sdk"
  },
  "homepage": "https://github.com/CaishenTech/caishen-sdk#readme",
  "bugs": {
    "url": "https://github.com/CaishenTech/caishen-sdk/issues"
  },
  "keywords": [
    "crypto",
    "wallet",
    "sdk",
    "caishen"
  ],
  "peerDependencies": {
    "zod": "3.x"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@langchain/core": "^0.3.44",
    "@types/axios": "^0.14.4",
    "ai": "^4.3.9",
    "axios": "^1.8.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "langchain": "^0.3.21",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "zod": "3.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mocha": "^7.0.2",
    "@types/node": "^22.5.5",
    "axios-mock-adapter": "^2.1.0",
    "env-var": "^7.5.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.0.0",
    "terser": "^5.37.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "tsx": "^4.19.3",
    "typescript": "^5.6.2"
  }
}
