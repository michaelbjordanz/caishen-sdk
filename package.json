{
  "name": "caishen-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "The Caishen SDK gives every agent or user access to unlimited multi-chain crypto wallets",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap --minify"
  },
  "author": "CaishenXYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:CaishenXYZ/caishen-sdk.git"
  },
  "keywords": [
    "crypto",
    "wallet",
    "sdk",
    "caishen"
  ],
  "peerDependencies": {
    "zod": "3.x"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.35.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^14.1.1",
    "@mysten/sui": "^1.21.1",
    "@mysten/sui.js": "^0.54.1",
    "@solana/web3.js": "^1.98.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.8.1",
    "bitcoinjs-lib": "^6.1.7",
    "bs58": "^6.0.0",
    "crypto": "^1.0.1",
    "ecpair": "^3.0.0-rc.0",
    "ethers": "^5.7.2",
    "near-api-js": "^5.0.1",
    "ripple-keypairs": "^2.0.0",
    "tiny-secp256k1": "^2.2.3",
    "tonweb": "^0.0.66",
    "tronweb": "^6.0.1",
    "tweetnacl": "^1.0.3",
    "zod": "3.x"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "terser": "^5.37.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.2"
  }
}
